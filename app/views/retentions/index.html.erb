<p id="notice"><%= notice %></p>

<h1>Retentions</h1>

<table>
  <thead>
    <tr>
      <th>Id retencion</th>
      <th>Numeroretencion</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @retentions.each do |retention| %>
      <tr>
        <td><%= retention.Id_Retencion %></td>
        <td><%= retention.NumeroRetencion %></td>
        <td><%= link_to 'Show', retention %></td>
        <td><%= link_to 'Edit', edit_retention_path(retention) %></td>
        <td><%= link_to 'Destroy', retention, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Retention', new_retention_path %>
