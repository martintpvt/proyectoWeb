<p id="notice"><%= notice %></p>

<h1>Entities</h1>

<table>
  <thead>
    <tr>
      <th>Id entidad</th>
      <th>Id usuario</th>
      <th>Identificacion</th>
      <th>Nombre</th>
      <th>Telefono</th>
      <th>Direccion</th>
      <th>Email</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entities.each do |entity| %>
      <tr>
        <td><%= entity.Id_Entidad %></td>
        <td><%= entity.Id_Usuario %></td>
        <td><%= entity.Identificacion %></td>
        <td><%= entity.Nombre %></td>
        <td><%= entity.Telefono %></td>
        <td><%= entity.Direccion %></td>
        <td><%= entity.Email %></td>
        <td><%= link_to 'Show', entity %></td>
        <td><%= link_to 'Edit', edit_entity_path(entity) %></td>
        <td><%= link_to 'Destroy', entity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity', new_entity_path %>
